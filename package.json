{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build:all": "concurrently \"yarn build\" \"yarn build:sit\"",
    "build": "umi build",
    "build:sit": "cross-env UMI_ENV=sit umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-layout": "^6.5.0",
    "@types/estree": "^0.0.50",
    "acorn": "^8.5.0",
    "lodash": "^4.17.21",
    "mobx": "^6.3.5",
    "mobx-react": "^7.2.1",
    "react": "17.x",
    "react-dom": "17.x",
    "react-use": "^17.3.1",
    "tapable": "^2.2.1",
    "typedi": "^0.10.0",
    "umi": "^3.5.20"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.5.20",
    "concurrently": "^6.3.0",
    "cross-env": "^7.0.3",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  }
}
